{
  "name": "owlet",
  "displayName": "Owlet",
  "description": "A VS Code theme modified from Night Owl",
  "version": "0.0.2",
  "publisher": "itsjonq",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/itsjonq/owlet"
  },
  "keywords": [
    "Theme",
    "Owlet",
    "Dark Theme",
    "Night Owl",
    "Night Theme",
    "Accessible Theme"
  ],
  "scripts": {
    "build": "node src/index.js",
    "start": "npm run build",
    "git:push": "git push --follow-tags",
    "postversion": "npm run release",
    "release": "vsce publish && npm run git:push"
  },
  "galleryBanner": {
    "color": "#1e222a",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "owlet.png",
  "contributes": {
    "themes": [
      {
        "label": "Owlet",
        "uiTheme": "vs-dark",
        "path": "./themes/owlet-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "lodash.template": "4.4.0"
  }
}
