{
  "name": "owlet",
  "displayName": "Owlet",
  "description": "A series of simple VSCode Theme",
  "version": "0.0.3",
  "publisher": "itsjonq",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/itsjonq/owlet"
  },
  "keywords": [
    "Theme",
    "Owlet",
    "Purple",
    "Palenight",
    "One",
    "Dark One",
    "Dark Theme",
    "Night Owl",
    "Night Theme"
  ],
  "scripts": {
    "build": "npm run clean && node src/index.js",
    "clean": "rm -rf themes",
    "start": "npm run build",
    "git:push": "git push --follow-tags",
    "postversion": "npm run release",
    "release": "vsce publish && npm run git:push"
  },
  "galleryBanner": {
    "color": "#1e222a",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.17.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "owlet.png",
  "contributes": {
    "themes": [
      {
        "label": "Owlet (Default)",
        "uiTheme": "vs-dark",
        "path": "./themes/owlet-theme-default.json"
      },
      {
        "label": "Owlet (Palenight)",
        "uiTheme": "vs-dark",
        "path": "./themes/owlet-theme-palenight.json"
      },
      {
        "label": "Owlet (Purple)",
        "uiTheme": "vs-dark",
        "path": "./themes/owlet-theme-purple.json"
      }
    ]
  },
  "devDependencies": {
    "glob": "7.1.3",
    "lodash.template": "4.4.0",
    "mkdirp": "0.5.1",
    "polished": "2.3.1"
  }
}
